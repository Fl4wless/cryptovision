// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./db.sqlite"
    // url      = env("DATABASE_URL")
}

model Example {
    id String @id @default(cuid())
}

model Contact {
    id String @id @default(cuid())
    firstName String
    userName String
    email String
    avatar String

}