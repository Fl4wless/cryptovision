    "dayjs": "^1.11.2",
    "formik": "^2.2.9",
    "hoist-non-react-statics": "^3.3.2",
    "next": "12.2.0",
    "next-auth": "^4.10.3",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@newhighsco/next-plugin-svgr": "^3.0.50",
